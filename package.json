{
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.5",
    "@auth0/auth0-react": "^2.2.4",
    "@reduxjs/toolkit": "^2.0.1",
    "antd": "^5.15.4",
    "axios": "^1.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "redux-thunk": "^3.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.7",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@openapitools/openapi-generator-cli": "^2.13.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^16.18.93",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "acorn": "^8.5.0",
    "ajv": "^8.12.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "less": "^3.9.0",
    "less-loader": "^10.0.1",
    "openapi-axios-mock-adapter": "^0.0.4",
    "openapi-merge-cli": "^1.3.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react-test-renderer": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "source-map-loader": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/__tests__/**/*.(test|spec).(ts|tsx)"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "testMatch": [
      "**/__tests__/**/*.(test|spec).(ts|tsx)"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+[^esm]\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$",
      "/node_modules/(?!(@bundled-es-modules)/)"
    ]
  },
  "license": "MIT",
  "name": "kwops_frontend",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "clean-files": "rm -rf ./output.swagger.json ./src/types/api",
    "eject": "react-scripts eject",
    "generate-openapi": "npm run clean-files && npm run merge-openapi && openapi-generator-cli generate",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "merge-openapi": "npx openapi-merge-cli",
    "test": "npm run generate-openapi && npm run lint && npm run ts:check && npm run test:only",
    "test:only": "jest --runInBand --detectOpenHandles --forceExit",
    "ts:check": "npx tsc --skipLibCheck"
  },
  "version": "0.0.1"
}
